
Routine Fibonacci(x)
    If(x < 2)
        Return x
    Else
        Return Fibonacci(x - 1) + Fibonacci(x - 2)
    EndIf
EndRoutine

Routine Main()
    Set i = 0
    Call Print("Calling Fibonacci(28) 5 times..\n")
    Set t = Clock()
    While(i < 5)
        Call Fibonacci(28.0)
        Set i = i + 1
    EndWhile
    Set t = Clock() - t
    Call Print("\nTook ", (t*1.0)/ClocksPerSecond, "s")
EndRoutine
