
Routine Fibonacci(x)
    If(x < 2)
        Return x
    Else
        Return Fibonacci(x - 1) + Fibonacci(x - 2)
    EndIf
EndRoutine

Routine Main()
    Print "Fibonacci(28) : "
    Set t = Clock()
    Print Fibonacci(28.0)
    Set t = Clock() - t
    Print "\nTook ", (t*1.0)/ClocksPerSecond, "s"
EndRoutine
