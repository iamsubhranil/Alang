Routine Main()
    Call Print("\nEnter first number : ")
    Input n1:Int
    Call Print("\nEnter second number : ")
    Input n2:Int
    Call Print("\n",n1," + ",n2," : ", n1 + n2)
    Call Print("\n",n1," - ",n2," : ", n1 - n2)
    Call Print("\n",n1," * ",n2," : ", n1 * n2)
    Call Print("\n",n1," / ",n2," : ", n1 / n2)
EndRoutine
