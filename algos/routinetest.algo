    Call Print("\nFact(",f,") : ",Fact(f))
    Call Print("\nBy default, each function returns : ", NoRet(7))
    Call LargeRoutine()
    Call Print("\nArbitraryTerminate(7) : ", ArbitraryTerminate(7))
EndRoutine
