Routine Foreign NativeFibonacci(x)
Routine Main()
    If(LoadLibrary("./libnfibo.so"))
        Call Print("\nTerm number : ")
        Input x:Int
        Call Print("\nNativeFibonacci(", x, ") : ", NativeFibonacci(x))
    Else
        Call Print("\n[Error] Unable to load the required library!")
    EndIf
EndRoutine
