Routine Main()
    Set size = 100000000
    Array list[size]
    Set time = Clock()
    Set i = 0
    While(i < size)
        Set list[i+1] = i, i = i + 1
    EndWhile
    Set sum = 0.0, i = 0
    While(i < size)
        Set sum = sum + list[i+1], i = i + 1
    EndWhile
    Set time = 1.0 * (Clock() - time)/ClocksPerSecond
    Call Print("\nsum : ", sum)
    Call Print("\nTook ", time, " seconds\n")
EndRoutine
