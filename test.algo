Begin
Set N = 24, i = N/2
While(i >= 2)
Begin
    If(N % i == 0)
    Then
        Set flag = 0
        Set j = 2
        While(j <= i/2)
        Begin
            If(i % j == 0)
            Then
                Set flag = 1
            EndIf
            Set j = j + 1
        EndWhile
        If(flag == 0)
        Then
            Print i, " is a prime factor of ", N, "\n"
        EndIf
    EndIf
    Set i = i - 1
EndWhile
End
